{
  "root": true,
  "extends": "eslint:recommended",
  "env": {
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-empty": "warn",
    "indent": [1,4]
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "arrowFunctions": true
    }
  }
}